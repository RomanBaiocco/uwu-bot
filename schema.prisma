// schema.prisma

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider             = "prisma-client-py"
  recursive_type_depth = 5
}

model Guilds {
  id             String  @id
  target         String?
  webhooks       Json    @default("{}")
  level          Int     @default(0)
}
